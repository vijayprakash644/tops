# Base URL for TopS.II API server (no trailing slash recommended)
TEST_BASE_URL=https://your-test-fqdn
PROD_BASE_URL=https://your-prod-fqdn

# Which environment index.php should use (TEST or PROD)
INDEX_ENV=TEST

# API keys for TopS.II (32-character alphanumeric)
TEST_API_KEY=replace_with_test_key
PROD_API_KEY=replace_with_prod_key

# Toggle upstream sending (false = log only)
ENABLE_REAL_SEND=false

# DB lookup retry sleep (seconds)
DB_LOOKUP_SLEEP_SECONDS=1
